<odoo>
  <data>


        <record id="student_registration_actions" model="ir.actions.act_window">
          <field name="name">Registration</field>
          <field name="res_model">student.registration</field>
          <field name="view_mode">tree,form,calendar,kanban</field>

        </record>

        <record id="res_partner_actions" model="ir.actions.act_window">
          <field name="name">Students</field>
          <field name="res_model">res.partner</field>
          <field name="view_mode">tree,form</field>
          <field name="domain">[('is_student', '=', True)]</field>


        </record>

        <record id="invoices_actions" model="ir.actions.act_window">
          <field name="name">Invoices</field>
          <field name="res_model">account.move</field>
          <field name="view_mode">tree,form</field>
        </record>



  </data>
</odoo>